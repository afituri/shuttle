    #dyn_1.panel.panel-default.toggle.panelMove.panelClose.panelRefresh
      // Start .panel
      .panel-heading
        h4.panel-title
          i.fa.fa-magic
          |  Form wizard with progressbar
        |                                     
        .panel-controls.panel-controls-right
          a.panel-refresh(href='#')
            i.fa.fa-circle-o
          a.toggle.panel-minimize(href='#')
            i.fa.fa-angle-up
          a.panel-close(href='#')
            i.fa.fa-times
      |                                     
